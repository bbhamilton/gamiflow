extends admin-layout.pug

block title
  title Gamiflow - Admin Panel - Dashboard

block content
  .container
    .row
      .col-sm-12
        .card-box.widget-inline
          .row
            .col-lg-3.col-sm-6
              .widget-inline-box.text-center
                h3.m-t-10
                  i.text-primary.mdi.mdi-access-point-network
                  b(data-plugin='counterup') 8954
                p.text-muted Lifetime total sales
            .col-lg-3.col-sm-6
              .widget-inline-box.text-center
                h3.m-t-10
                  i.text-custom.mdi.mdi-airplay
                  b(data-plugin='counterup') 7841
                p.text-muted Income amounts
            .col-lg-3.col-sm-6
              .widget-inline-box.text-center
                h3.m-t-10
                  i.text-info.mdi.mdi-black-mesa
                  b(data-plugin='counterup') 6521
                p.text-muted Total users
            .col-lg-3.col-sm-6
              .widget-inline-box.text-center.b-0
                h3.m-t-10
                  i.text-danger.mdi.mdi-cellphone-link
                  b(data-plugin='counterup') 325
                p.text-muted Total visits
    // end row
    .row
      .col-lg-6
        .card-box
          h4.m-t-0 Total Revenue
          .text-center
            ul.list-inline.chart-detail-list
              li
                h5.font-normal
                  i.fa.fa-circle.m-r-10.text-primary
                  | Series A
              li
                h5.font-normal
                  i.fa.fa-circle.m-r-10.text-muted
                  | Series B
          #dashboard-bar-stacked(style='height: 300px;')
      // end col
      .col-lg-6
        .card-box
          h4.m-t-0 Sales Analytics
          .text-center
            ul.list-inline.chart-detail-list
              li
                h5.font-normal
                  i.fa.fa-circle.m-r-10.text-primary
                  | Mobiles
              li
                h5.font-normal
                  i.fa.fa-circle.m-r-10.text-info
                  | Tablets
          #dashboard-line-chart(style='height: 300px;')
      // end col
    // end row
    .row
      .col-sm-12
        .card-box
          h4.m-t-0 Contacts
          .table-responsive
            table.table.table-hover.mails.m-0.table.table-actions-bar
